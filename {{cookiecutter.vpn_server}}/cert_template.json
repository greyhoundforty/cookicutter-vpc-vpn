{
    "config_type": "private_cert_configuration_template",
    "name": "{{ cookiecutter.name }}-cert-template",
    "certificate_authority": "{{ cookiecutter.name }}-intermediate-ca",
    "max_ttl": "{{ cookiecutter.server_max_ttl }}",
    "ttl": "{{ cookiecutter.server_ttl }}",
    "allow_localhost": true,
    "allow_any_name": true,
    "allow_ip_sans": false,
    "allow_subdomains": false,
    "allow_wildcard_certificates": false,
    "enforce_hostnames": false,
    "server_flag": true,
    "client_flag": false,
    "code_signing_flag": false,
    "email_protection_flag": false,
    "key_bits": 4096,
    "key_type": "rsa",
    "use_csr_common_name": false,
    "use_csr_sans": false,
    "organization": ["{{ cookiecutter.organization }}"],
    "country": ["{{ cookiecutter.country }}"],
    "locality": ["{{ cookiecutter.locality }}"],
    "province": ["{{ cookiecutter.province }}"],
    "ou": ["{{ cookiecutter.ou }}"]
  }